{
      "version": 2,
      "builds": [
        {
          "src": "index.html",
          "use": "@vercel/static"
        },
        {
          "src": "*.jsx",
          "use": "@vercel/node"
        }
      ],
      "routes": [
        {
          "src": "/api/register",
          "dest": "/api/register.js"
        },
        {
          "src": "/(.*)",
          "dest": "/index.html"
        }
      ]
    }